<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_move_form_inherit_inclusive_price" model="ir.ui.view">
        <field name="name">account.move.form.inherit.price.with.tax</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
